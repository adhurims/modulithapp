@model IEnumerable<CustomerManagement.Domain.Identity.ApplicationUser>

<h2>Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Email</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>
                    <a asp-action="EditUser" asp-route-id="@user.Id" class="btn btn-sm btn-warning">Edit</a>
                   
                </td>
            </tr>
        }
    </tbody>
</table>
